{
  "name": "nextjs-16-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "checks": "chmod +x $(git rev-parse --show-toplevel)/scripts/checks.sh && $(git rev-parse --show-toplevel)/scripts/checks.sh",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./src/db/drizzle.config.ts",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint",
    "pipeline": "chmod +x $(git rev-parse --show-toplevel)/scripts/pipeline.sh && $(git rev-parse --show-toplevel)/scripts/pipeline.sh",
    "start": "next start",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-slot": "1.2.4",
    "@t3-oss/env-nextjs": "0.13.10",
    "better-auth": "1.4.18",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-orm": "0.45.1",
    "lucide-react": "0.563.0",
    "next": "16.1.6",
    "pg": "8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "tailwind-merge": "3.4.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@jackjakarta/prettier-config": "0.3.2",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "25.2.3",
    "@types/pg": "8.16.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "drizzle-kit": "0.31.8",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@9.15.3"
}
